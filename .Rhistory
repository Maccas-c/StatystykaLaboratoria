wspolczynnik_zmiennosci <- function(x, na.rm = FALSE) {
if(is.numeric(x) == TRUE) {
if(na.rm == TRUE) {
x <- x[!is.na(x)]
print
sd <- sd(x)
avg <- mean(x)
V <- sd/avg
return(V* 100)
} else {
return(NA)
}
} else {
stop("argument nie jest liczbą")
}
}
